{
  "name": "moviexp",
  "version": "0.0.1",
  "description": "explore movies from omdb API",
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 6565",
    "build": "DEPLOYMENT_PLATFORM=vercel astro build",
    "build:local": "astro build",
    "start:local": "astro preview --port 5656",
    "logic-test": "pnpm node-ts ./scripts/logic-test.ts",
    "lint": "eslint . --ext .js,.ts,.tsx --color",
    "node-ts": "node --loader esbuild-register/loader -r esbuild-register",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RofiSyahrul/moviexp.git"
  },
  "keywords": [
    "movies",
    "omdb API",
    "explore movies",
    "svelte",
    "astro",
    "typescript"
  ],
  "author": "Rofi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RofiSyahrul/moviexp/issues"
  },
  "homepage": "https://moviexp.rofi.link",
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/ua-parser-js": "^0.7.37",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.3.1",
    "esbuild": "0.19.5",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-lube": "^0.2.5",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.34.0",
    "globby": "13.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.63.3",
    "standard-version": "^9.5.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "svelte": "^4.2.1",
    "svelte-eslint-parser": "^0.33.1",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@astrojs/node": "^6.0.3",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@astrojs/vercel": "^5.0.2",
    "astro": "^3.3.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "sharp": "^0.32.6",
    "ua-parser-js": "^1.0.36"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,svelte,astro}": [
      "eslint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
